package model;

public class User {

	private String email;
	private String password;
	
	public User(String email, String password) {
		super();
		this.email = email;
		this.password = password;
	}

	public User(String line) {
		super();
		this.email = line.split(";")[0];
		this.password = line.split(";")[1];
	}
	
	
}
